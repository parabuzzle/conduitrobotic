<h1>Login</h1>
<div class="lightenup">
<table>
	<%=form_for :user do |f|%>
		<tr>
			<td>Email:</td>
			<td><%=f.text_field(:email)%></td>
		</tr>
		<tr>
			<td>Password:</td>
			<td><%=f.password_field(:password)%></td>
		</tr>
		<tr>
			<td colspan="2"><%=f.submit("Login")%></td>
		</tr>
	<%end%>
</table>
</div>